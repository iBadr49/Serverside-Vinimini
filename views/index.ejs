<%- include('./partials/head.ejs') %>
<article>
   
       
           <input type="text" class="searchTerm" placeholder="Zoek hier je boek">
           <button type="submit" class="searchButton"> </button>
       
     
<h1>Boeken overzicht</h1>
</article>  
<section class="boekenOverzicht">

<% results.forEach(result => { %>
 
    <article class="boek">
        <img src="<%= result.coverimages %>" alt="boek">
       <h2> <%= result.titles[0] %></h2>
    </article>

   <% }) %>
</section>

<%- include('./partials/foot.ejs') %>